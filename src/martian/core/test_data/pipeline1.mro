@include "stages.mro"


pipeline AWESOME1(
        in string data,
        out string final,
){
        call GETDATA(
                stuffG = self.data,
        )
        call SORT(
                stuffS = GETDATA.output1,
        )

        call COUNT(
                stuffC = SORT.output1,
        )
        call COLLATE(
                stuffO1 = COUNT.output1,
                stuffO2 = COUNT.output1,
        )
        return (
                final = COLLATE.output1,
        )
}

        
